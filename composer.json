{
    "name": "nystronsolar/growattspreadsheet",
    "description": "Read, Write and Manage Growatt Device Data Spreadsheets",
    "license": "GPL-3.0-only",
    "homepage": "https://github.com/NystronSolar/GrowattSpreadsheet",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "type": "library",
    "require": {
        "php": ">=8.2",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-gd": "*",
        "ext-dom": "*",
        "phpoffice/phpspreadsheet": "^1.27"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.7",
        "friendsofphp/php-cs-fixer": "^3.14",
        "symfony/var-dumper": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "NystronSolar\\GrowattSpreadsheet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "TheDevick",
            "email": "erick@bilhalba.com.br",
            "homepage": "https://TheDevick.vercel.app"
        }
    ]
}
